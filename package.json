{
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "browser-sync": "^3.0.2",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "server": "node server.js",
    "sync": "browser-sync start --proxy localhost:3000 --files *.html styles/style.css",
    "start": "npm-run-all --parallel server sync",
    "sass": "node-sass --watch styles/style.scss -o styles/style.css"
  }
}
